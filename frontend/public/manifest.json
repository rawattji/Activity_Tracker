{
  "manifest_version": 3,
  "name": "Activity Tracker",
  "description": "Analyse User's web activity",
  "version": "1.0.0",
  "permissions": [
    "storage",
    "tabs",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Open"
  },
  "web_accessible_resources": [
    {
      "resources": ["*.js", "*.css"],
      "matches": ["<all_urls>"]
    }
  ]
}
